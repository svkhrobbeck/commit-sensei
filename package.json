{
  "name": "commit-sensei",
  "module": "src/main.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "clean": "rm -rf dist",
    "start:local": "bun src/main.ts",
    "dev": "bun run --watch src/main.ts",
    "build": "bun clean && tsc",
    "typecheck": "tsc --noEmit true",
    "lint": "eslint . --quiet --max-warnings 0",
    "format": "prettier --write \"**/*.{json,ts,tsx,md,scss,css,html}\"",
    "stop": "pm2 stop ecosystem.config.cjs",
    "start": "pm2 start ecosystem.config.cjs",
    "restart": "pm2 restart ecosystem.config.cjs"
  },
  "dependencies": {
    "@grammyjs/ratelimiter": "^1.2.1",
    "@octokit/rest": "^22.0.1",
    "axios": "^1.13.2",
    "chalk": "^5.6.2",
    "cors": "^2.8.5",
    "dedent": "^1.7.1",
    "grammy": "^1.39.2",
    "hono": "^4.11.3",
    "http-status-codes": "^2.3.0",
    "lodash": "^4.17.21",
    "node-cron": "^4.2.1",
    "nodemon": "^3.1.11",
    "octokit": "^5.0.5",
    "utilzify": "^1.8.2",
    "zutsheet": "^2.6.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@octokit/types": "^16.0.0",
    "@types/bun": "latest",
    "@types/lodash": "^4.17.21",
    "@types/node": "^25.0.3",
    "@types/node-cron": "^3.0.11",
    "eslint": "^9.39.2",
    "globals": "^17.0.0",
    "jiti": "^2.6.1",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.51.0"
  }
}
