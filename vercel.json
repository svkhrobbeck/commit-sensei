{
  "installCommand": "yarn --omit=dev",
  "buildCommand": "yarn build",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    },
    {
      "src": "api/*.ts",
      "use": "@vercel/node"
    }
  ],
  "outputDirectory": "dist",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/server.ts"
    }
  ]
}
